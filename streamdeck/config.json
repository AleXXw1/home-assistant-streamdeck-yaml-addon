{
    "name": "home-assistant-streamdeck-yaml",
    "version": "dev",
    "slug": "home-assistant-streamdeck-yaml",
    "description": "home-assistant-streamdeck-yaml add-on by Bas Nijholt",
    "url": "https://github.com/basnijholt/home-assistant-streamdeck-yaml-addon",
    "startup": "before",
    "hassio_api": true,
    "hassio_role": "default",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "boot": "auto",
    "ingress": true,
    "ports": {
        "websocket_port/tcp": null
    },
    "ports_description": {
        "websocket_port/tcp": "home-assistant-streamdeck-yaml WebSocket port"
    },
    "devices": [
        "/dev/bus/usb:/dev/bus/usb:rwm"
    ],
    "options": {
        "hass_host": "192.168.1.24",
        "hass_token": "...",
        "streamdeck_config": "/config/streamdeck.yaml",
        "websocket_protocol": "wss"
    },
    "panel_icon": "mdi:radiator",
    "schema": {
        "hass_host": "str",
        "hass_token": "str",
        "streamdeck_config": "str",
        "websocket_protocol": "str"
    }
}
